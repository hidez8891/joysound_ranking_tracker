<h1>Listing logs</h1>

<table>
  <tr>
    <th>Song</th>
    <th>Rank</th>
    <th>Point</th>
    <th>Date</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @logs.each do |log| %>
  <tr>
    <td><%= log.song_id %></td>
    <td><%= log.rank %></td>
    <td><%= log.point %></td>
    <td><%= log.date %></td>
    <td><%= link_to 'Show', log %></td>
    <td><%= link_to 'Edit', edit_log_path(log) %></td>
    <td><%= link_to 'Destroy', log, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Log', new_log_path %>
